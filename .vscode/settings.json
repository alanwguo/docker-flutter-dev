{
    "dart.analyzerAdditionalArgs": [
        "--enable-completion-model"
    ],
    "editor.suggestSelection": "first",
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".idea": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "build": true,
        ".dart_tool": true,
    }
}
}